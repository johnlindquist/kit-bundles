{
  "name": "@johnlindquist/kit-internal",
  "author": "John Lindquist (https://johnlindquist.com)",
  "license": "ISC",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "Shaving off npm install size of Kit",
  "type": "module",
  "typesVersions": {
    ">=0": {
      "*": [
        "types/*"
      ]
    }
  },
  "exports": {
    "./*": {
      "require": "./dist/*.cjs",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist/*",
    "types/*"
  ],
  "scripts": {
    "test": "ava",
    "build-dist": "node ./build-dist.js",
    "build-types": "node ./build-types.js",
    "build": "concurrently npm:build-*"
  },
  "devDependencies": {
    "@octokit/auth-oauth-device": "^4.0.4",
    "@octokit/core": "^4.2.0",
    "@octokit/plugin-paginate-rest": "^6.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^7.0.1",
    "@octokit/plugin-retry": "^4.1.3",
    "@octokit/plugin-throttling": "^5.1.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/project-name-generator": "^2.1.1",
    "concurrently": "^8.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "dts-bundle-generator": "^8.0.1",
    "enquirer": "^2.3.6",
    "fs-extra": "^11.1.1",
    "lowdb": "^6.0.1",
    "minimist": "^1.2.8",
    "project-name-generator": "^2.1.9",
    "rollup": "^3.21.4",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rxjs": "^7.8.1",
    "scriptkit-octokit": "^1.0.4",
    "semantic-release": "^21.0.2",
    "semantic-release-plugin-update-version-in-files": "^1.1.0",
    "strip-ansi": "^7.0.1",
    "typescript": "^5.0.4"
  },
  "release": {
    "branches": [
      "+([0-9]).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  }
}
