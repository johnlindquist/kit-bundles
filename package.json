{
  "name": "@johnlindquist/kit-bundles",
  "author": "John Lindquist (https://johnlindquist.com)",
  "license": "ISC",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "MinifiedScript Kit bundles",
  "type": "module",
  "exports": {
    "./*": "./bundles/*.js"
  },
  "files": [
    "./bundles/*"
  ],
  "devDependencies": {
    "date-fns": "^2.25.0",
    "esbuild": "^0.13.8",
    "rxjs": "^7.4.0",
    "semantic-release": "^18.0.0",
    "semantic-release-plugin-update-version-in-files": "^1.1.0"
  },
  "release": {
    "branches": [
      "+([0-9]).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  }
}
